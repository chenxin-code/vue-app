.design-css {
  .inner-div {
    position relative;
    width 100%;
    height 100%;
    padding 45px 15px 10px 15px;
    .status-bar {
      position: absolute;
      left: 0px;
      right: 0px;
      top: 44px;
      text-align: center;
      height: 20px;
      color: white;
      font-size: 14px;
      z-index: 99;
      img {
        height: 20px;
      }
    }
    .title-div {
      position absolute;
      left 15px;
      right 15px;
      top 5px;
      padding 5px 0px;
      img {
        float right;
        width 30px;
        height 30px;
        background-color #bbb
        border-radius 15px;
      }
      span {
        display inline-block;
        padding 8px 0px;
        font-size 14px;
      }
    }
    .white-div {
      width 100%;
      height 100%;
      overflow hidden;
      background-color #FFFFFF;
      .scroll-div {
        width 100%;
        height 100%;
        overflow-x: hidden;
        overflow-y auto;
        .space-div {
          width 100%;
          height 64px;
        }
      }
    }
  }
  .eidt-div {
    position absolute
    left 405px;
    top 0px;
    bottom 0px;
    width 495px;
    .white-div {
      padding 10px;
    }
    .type {
      width 100%;
      .edit-right-div {
        float right;
        width 280px;
        .item-div {
          float left;
          width 100px;
          height 60px;
        }
      }
    }
    .default-div {
      width 100%;
      .form-div {
        padding 0 3% 10px;
      }
    }
    .edit-module-title {
      text-align center;
      line-height 40px;
      height 40px;
    }
    .edit-label {
      padding 10px 15px;
      display inline-block;
      vertical-align middle;
    }
    .edit-tip {
      font-size 12px;
      color $color-text-gray
      padding 10px;
      text-align center;
    }
    .edit-left-space {
      margin-left 24px;
    }
    .edit-all-line {
      margin-top 4px;
      width 100%;
    }
    .edit-line {
      display block;
      margin-left 3%
      height 1px;
      width 94%;
      background-color $color-border
    }
    .bai-block {
      margin-top 10px;
      padding 10px;
      //width 94%;
      border: 1px dashed #dcdddd;
      overflow hidden;
      .image-div {
        position relative
        float left;
        width 60px;
        height 80px;
        text-align center;
        margin-right 10px;
        img {
          display block;
          width 60px;
          height 60px;
          background #efefef;
        }
        span {
          display block;
          padding 4px;
          font-size 12px;
        }
      }
      .right-label {
        display inline-block;
        vertical-align middle;
      }
    }
    .block-img {
      display: block;
    }
    .active {
      border: 1px solid #38f;
    }
    .tip-p {
      position absolute;
      left 0px;
      right 0px;
      bottom 0px;
      text-align center;
      padding 6px;
      background-color rgba(255, 255, 255, 0.3)
    }
    .tip-p-1 {
      bottom 20px;
    }
    .good-tip {
      position absolute;
      left 10%;
      right 10%;
      top 50%;
      margin-top -10px;
      height 20px;
      text-align center;
      font-size 14px;
      color #999;
    }
  }
}

