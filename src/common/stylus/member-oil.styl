@import 'variable.styl';


.member-moudle-title-activity{
  margin-top 30px
  margin-left 5px
  margin-bottom 10px
  color black
  font-size $font-size-large
  font-weight bold
}

.member-moudle-title {
  margin-top 30px
  margin-left 30px
  margin-bottom 10px
  color black
  font-size $font-size-large
  font-weight bold
}

.space {
  width 100%
  height 10px
  background $color-background-lll
}

.hr-class {
  width 100%
  margin-left 10px
  margin-right 10px
  margin-top 20px
  border-top: 1px solid #ddd;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.member-all-oil-title {
  height 50px
  line-height 50px
  width 100%
  text-align center
  color black
  font-size $font-size-large
  font-weight bold
}

